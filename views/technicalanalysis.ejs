    <script src= "https://cdn.zingchart.com/zingchart.min.js"></script>
    <script> zingchart.MODULESDIR = "https://cdn.zingchart.com/modules/";
    ZC.LICENSE = ["569d52cefae586f634c54f86dc99e6a9","ee6b7db5b51705a13dc2339db3edaf6d"];</script>
    <script>
    var risk = <%= risk %>;
    var sPrice = <%= startPrice %>;
    var tempVar = (risk/sPrice) * 100;
    var finalVar;

    if(tempVar >= 35)
    finalVar = 5;
    else if(tempVar >= 30)
    finalVar = 24;
    else if(tempVar >= 20)
    finalVar = 40;
    else if(tempVar >= 15)
    finalVar = 60;
    else
    finalVar = 75;
    var myConfig8 = {
      "type":"gauge",
      "scale-r":{
        "aperture":200,
        "values":"0:80:20",
        "center":{
          "size":10,
          "background-color":"#66CCFF #FFCCFF",
          "border-color":"none"
        },
        "ring":{
          "size":15,
          "rules":[
            {
              "rule":"%v >= 0 && %v <= 20",
              "background-color":"red"
            },
            {
              "rule":"%v >= 20 && %v <= 40",
              "background-color":"orange"
            },
            {
              "rule":"%v >= 40 && %v <= 60",
              "background-color":"yellow"
            },
            {
              "rule":"%v >= 60 && %v <= 80",
              "background-color":"green"
            },
            {
              "rule":"%v >= 80 && %v <=100",
              "background-color":"blue"
            }
          ]
        },
        "labels":["Very High Risk","High Risk","Neutral","Low Risk","Very Low Risk"],  //Scale Labels
        "item":{  //Scale Label Styling
          "font-color":"purple",
          "font-family":"Georgia, serif",
          "font-size":11,
          "font-weight":"bold",   //or "normal"
          "font-style":"normal",   //or "italic"
          "offset-r":0,
          "angle":""
        }
      },
      "plot":{
        "csize":"5%",
        "size":"100%",
        "background-color":"#000000",
        "animation":{ //Animation object
          "effect":2,
          "method":3,
          "sequence":1,
          "speed":3000
        }
      },
      "series":[
        {"values":[finalVar]}
      ]
    };

    zingchart.render({
      id : 'myChart',
      data : myConfig8,
      height : "70%",
      width: "85%"
    });

    </script>
